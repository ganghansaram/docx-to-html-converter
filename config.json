{
  "style_mapping": {
    "by_style": {
      "제목 1": "h1",
      "제목 2": "h2",
      "제목 3": "h3",
      "Heading 1": "h1",
      "Heading 2": "h2",
      "Heading 3": "h3",
      "Title": "h1",
      "Normal": "p",
      "본문": "p"
    },
    "by_font_size": {
      "24": "h1",
      "22": "h1",
      "18": "h2",
      "16": "h3",
      "14": "h3",
      "default": "p"
    },
    "priority": "style_first"
  },
  "text_formatting": {
    "bold": "strong",
    "italic": "em",
    "underline": "u",
    "strikethrough": "del",
    "subscript": "sub",
    "superscript": "sup"
  },
  "special_blocks": {
    "note": ["NOTE", "참고", "비고"],
    "caution": ["CAUTION", "주의"],
    "warning": ["WARNING", "경고"]
  },
  "options": {
    "remove_headers_footers": true,
    "extract_images": true,
    "remove_empty_paragraphs": true,
    "convert_smart_quotes": true
  },
  "output": {
    "fragment_only": true,
    "encoding": "utf-8",
    "indent": true
  }
}
